{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-playground/src",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "web-client": {
      "type": "library",
      "root": "libs/web-client",
      "entryFile": "index",
      "sourceRoot": "libs/web-client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/web-client/tsconfig.lib.json"
      }
    },
    "file-manager": {
      "type": "library",
      "root": "libs/file-manager",
      "entryFile": "index",
      "sourceRoot": "libs/file-manager/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file-manager/tsconfig.lib.json"
      }
    },
    "event-publisher": {
      "type": "library",
      "root": "libs/event-publisher",
      "entryFile": "index",
      "sourceRoot": "libs/event-publisher/src",
      "compilerOptions": {
        "tsConfigPath": "libs/event-publisher/tsconfig.lib.json"
      }
    },
    "file-storage": {
      "type": "library",
      "root": "libs/file-storage",
      "entryFile": "index",
      "sourceRoot": "libs/file-storage/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file-storage/tsconfig.lib.json"
      }
    },
    "event-listener": {
      "type": "library",
      "root": "libs/event-listener",
      "entryFile": "index",
      "sourceRoot": "libs/event-listener/src",
      "compilerOptions": {
        "tsConfigPath": "libs/event-listener/tsconfig.lib.json"
      }
    }
  }
}